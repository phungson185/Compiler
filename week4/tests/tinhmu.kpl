Program tinhmu;
var input : integer;
    output : integer;
Function tinhmu(n : integer) : integer;
var i : integer;
begin
  output := 1;
  FOR i := 1 TO n DO
    begin
        output := 2 * output;
    end;
    tinhmu := output;
end;

begin
    input := readi;
    call WriteI(tinhmu(input));
end.
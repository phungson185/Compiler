Program switchcase;
var input : integer;


Function cd(i : integer) : char;
var kq : char;
begin
    switch i
    begin
        case 10  : kq := 'A'; break ;
        case 9  : kq := 'A';break ;
        case 8  : kq := 'A';break ;
        case 7  : kq := 'B';break ;
        case 6  : kq := 'B';break ;
        case 5  : kq := 'C';break ;
        case 4  : kq := 'C';break ;
        case 3  : kq := 'D';break ;
        case 2  : kq := 'D';break ;
        case 1  : kq := 'F';break ;
    end;
end;


begin
    input := readi;
    call writeC(cd(input));
end.